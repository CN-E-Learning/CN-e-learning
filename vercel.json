{
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "docs/.vitepress/dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/docs/.vitepress/dist/$1"
    },
    {
      "src": "/CN-e-learning/(.*)",
      "dest": "/docs/.vitepress/dist/$1"
    }
  ],
  "cleanUrls": true,
  "trailingSlash": false
}
